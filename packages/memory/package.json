{
  "name": "@skillkit/memory",
  "version": "1.8.1",
  "description": "CozoDB-backed semantic memory with embeddings for SkillKit",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@skillkit/core": "workspace:*",
    "cozo-node": "^0.7.6",
    "@xenova/transformers": "^2.17.2"
  },
  "devDependencies": {
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  },
  "engines": {
    "node": ">=18"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rohitg00/skillkit.git",
    "directory": "packages/memory"
  },
  "keywords": [
    "skillkit",
    "memory",
    "cozodb",
    "embeddings",
    "semantic-search"
  ],
  "license": "Apache-2.0"
}
