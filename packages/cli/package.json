{
  "name": "@skillkit/cli",
  "version": "1.9.0",
  "description": "CLI commands for SkillKit",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
